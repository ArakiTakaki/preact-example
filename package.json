{
  "name": "preact-example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint src/",
    "dev": "webpack --config ./webpack.develop.js --watch",
    "prod": "webpack --config ./webpack.production.js",
    "production": "webpack --config ./webpack.production.js",
    "mock": "yarn concurrently \"yarn mock_backend\" \"yarn mock_frontend\"",
    "mock_frontend": "webpack-dev-server --config ./webpack.mock.js --mode development --open",
    "mock_backend": "json-server --watch mock/db.json --port 3001"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^6.0.0",
    "mobx": "^5.5.0",
    "mobx-preact": "^3.0.0",
    "preact": "^8.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-preact": "^1.1.0",
    "concurrently": "^4.0.1",
    "eslint": "^5.6.1",
    "eslint-config-google": "^0.10.0",
    "eslint-plugin-react": "^7.11.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "json-server": "^0.14.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}
